{
  "parserOptions": {
    "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
  },
  "environment": {
    "browser": true
  },
  "ecmaFeatures": {
  },
  "rules": {
    "strict": 0,
    "comma-style": 0,
    "quotes": [1, "single", "avoid-escape"],
    "no-extra-bind": 2,
    "eqeqeq": 2,
    "comma-dangle": 0,
    "no-cond-assign": [2, "except-parens"],
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 1,
    "no-debugger": 2,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 1,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 2,
    "no-unreachable": 1,
    "use-isnan": 1,
    "valid-jsdoc": 2,
    "valid-typeof": 1,
    "no-unexpected-multiline": 1
  }
}
