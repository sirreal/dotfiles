# Upgrade Vundle plugins
function vimupdate {
  vim +PluginInstall! +qall
}

function nvimupdate {
  nvim +PluginInstall! +qall
}

# Serve static directory (python)
function serveit {
  python -m SimpleHTTPServer 9090
}

# Serve static directory (php)
function serveitphp {
  php -S localhost:9090
}
